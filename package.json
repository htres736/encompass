{
  "name": "encompass",
  "version": "4.0.0",
  "private": true,
  "description": "enCoMPASS",
  "keywords": [
    "assessment",
    "rubrics",
    "education"
  ],
  "homepage": "https://github.com/mathematicalthinking/encompass#readme",
  "bugs": {
    "url": "https://github.com/mathematicalthinking/encompass/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathematicalthinking/encompass.git"
  },
  "license": "AGPL-3.0",
  "author": "Mathematical Thinking",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "dev": "concurrently \"npm:server\" \"npm:client\"",
    "test": "concurrently \"npm:server-test\" \"npm:client-test\"",
    "test:pc": "concurrently \"npm:server-test:pc\" \"npm:client-test\"",
    "client": "ember server --proxy=http://localhost:8080",
    "client-test": "ember server --port=8083 --proxy=http://localhost:8082",
    "server": "nodemon app_server/server.js",
    "server-test": "NODE_ENV=seed nodemon app_server/server.js",
    "server-test:pc": "\"SET NODE_ENV=seed\"&& nodemon app_server/server.js",
    "selenium": "./node_modules/.bin/mocha test/selenium/$npm_config_test",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test:ember": "ember test --server",
    "seed": "node test/seedDb",
    "drop-testdb": "echo 'db.dropDatabase()' | mongo encompass_test",
    "seed-vmt": "cd ../vmt/server && npm run test",
    "seed-mt": "cd ../mt-sso && npm run test",
    "seed-all": "concurrently \"grunt serve-seed\" \"npm:seed-vmt\" \"npm:seed-mt\"",
    "restore-mt": "cd ../mt-sso && npm run seed",
    "restore-vmt": "cd ../vmt/server && npm run seed",
    "dev-vmt": "cd ../vmt/server && npm run dev",
    "dev-mt": "cd ../mt-sso && npm run dev",
    "dev-all": "concurrently \"npm:dev\" \"npm:dev-mt\" \"npm:dev-vmt\"",
    "test-travis": "NODE_ENV=seed node server/server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie": "^0.4.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "debug": "^4.3.4",
    "dotenv": "^8.2.0",
    "express": "^4.19.2",
    "express-paginate": "^1.0.2",
    "htmlparser2": "^6.0.1",
    "http": "0.0.0",
    "http-errors": "^1.8.0",
    "i": "^0.3.6",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^8.5.1",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "moment": "^2.30.1",
    "mongoose": "^5.13.22",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nconf": "^0.11.4",
    "nodemailer": "^6.9.13",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "pdf2pic": "^2.2.4",
    "q": "^1.5.1",
    "randomcolor": "^0.6.2",
    "request": "^2.88.2",
    "selectize": "^0.12.6",
    "selenium-webdriver": "^4.21.0",
    "sharp": "^0.29.2",
    "socket.io": "^2.5.0",
    "sweetalert2": "^11.12.4",
    "underscore": "^1.13.6",
    "uuid": "^8.3.2",
    "wordcloud": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@ember/jquery": "^1.1.0",
    "@ember/optional-features": "^1.3.0",
    "@ember/render-modifiers": "^2.1.0",
    "@ember/test-helpers": "^2.9.4",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.4.1",
    "chai-http": "^4.4.0",
    "chromedriver": "^99.0.0",
    "concurrently": "^6.0.1",
    "ember-auto-import": "^2.7.2",
    "ember-cli": "^4.4.1",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-custom-properties": "^2.0.2",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-favicon": "^2.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mdc-sass": "^2.0.19",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-cookies": "^0.5.2",
