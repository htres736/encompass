    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-lodash": "^4.19.5",
    "ember-modifier": "^3.2.7",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.1.0",
    "ember-source": "^4.4.5",
    "ember-table": "^4.0.0",
    "ember-template-lint": "^4.8.0",
    "ember-unused-components": "^1.2.2",
    "ember-welcome-page": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "geckodriver": "^2.0.1",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "qunit": "^2.21.0",
    "qunit-dom": "^2.0.0",
    "sass": "^1.77.3",
    "webpack": "^5.91.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "nodemonConfig": {
    "watch": [
      "app_server/*"
    ]
  }
}
